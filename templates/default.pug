include includes/iconGithub.pug
include includes/iconYouTube.pug
include includes/icon.pug


doctype html
html(lang="en")
    head
        title= pageTitle

        link(rel='stylesheet', href='/styles.css')

        block header

    body
        include includes/navbar.pug

        .container
            .row
                #content
                    block content
                        h1= title

                        +iconGithub(github)
                        +iconYouTube(youtube)

                        != contents

        #footer
            block footer
