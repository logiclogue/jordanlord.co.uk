extends default.pug

block content
    .twelve
        img.u-full-width.hidden#sourceImage(src='/res/jordan.png')

    .twelve
        canvas.u-full-width#myCanvas(width='256', height='192', style={ 'image-rendering': 'pixelated' })
    
    .row.margin-bottom
        .home-icons
            .three.columns
                +iconGithub(github)

            .three.columns
                +icon('https://youtube.com/channel/', youtube, 'youtube')

            .three.columns
                +icon('https://linkedin.com/in/', linkedin, 'linkedin')

            .three.columns
                +icon('https://twitter.com/', twitter, 'twitter')

    != contents
